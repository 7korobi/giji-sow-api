    "debug": "~2.6.9",
    "decaffeinate": "^6.1.9",
    "express": "^4.17.3",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^3.4.0",
    "gulp-brotli": "^3.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-load-plugins": "^2.0.7",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-shell": "^0.8.0",
    "http-errors": "^2.0.0",
    "mongodb-bluebird": "^0.1.2",
